    "@achrinza/node-ipc": "^10.1.7",
    "@vue/reactivity": "^3.2.45",
    "address": "^1.2.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "colord": "^2.9.3",
