{
  "name": "commas",
  "version": "0.29.2",
  "productName": "Commas",
  "author": "CyanSalt",
  "description": "A hackable terminal and command runner.",
  "main": "dist/main/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "postinstall": "electron-rebuild",
    "dev": "node build/build-git.mjs && electron .",
    "dev:all": "node build/build.mjs && electron .",
    "dev:user": "node build/build-user.mjs && electron .",
    "build": "node build/build.mjs && node build/pack.mjs -- --local",
    "pack": "node build/build.mjs && node build/pack.mjs",
    "release": "dotenv release-it --",
    "lint:script": "eslint --ignore-path .gitignore --ext .js,.cjs,.mjs,.ts,.vue --cache --fix .",
    "lint:style": "stylelint --ignore-path .gitignore --cache --fix ./**/*.{css,scss,vue}",
    "lint": "npm run lint:script && npm run lint:style"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/commas.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/commas/issues"
  },
  "homepage": "https://github.com/CyanSalt/commas#readme",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@commas/api": "file:api",
    "@cyansalt/eslint-config-preset": "^1.15.0",
    "@cyansalt/stylelint-config-preset": "^1.4.1",
    "@cyansalt/tsconfig": "^2.2.1",
    "@electron/rebuild": "^3.3.0",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/fuzzaldrin-plus": "^0.6.4",
    "@types/lodash": "^4.14.200",
    "@types/node": "^18.18.6",
    "@types/node-ipc": "^9.2.2",
    "@types/shell-quote": "^1.7.3",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue-macros/reactivity-transform": "^0.3.23",
    "css-font-face-src": "^1.0.0",
    "devicon": "^2.15.1",
    "dotenv-cli": "^7.3.0",
    "electron": "^26.4.1",
    "electron-packager": "^17.1.2",
    "esbuild": "^0.19.5",
    "lucide-static": "^0.288.0",
    "png2icons": "^2.0.1",
    "release-it": "^16.2.1",
    "sass": "^1.69.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "xterm": "^5.3.0",
    "xterm-addon-canvas": "^0.5.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-image": "^0.5.0",
    "xterm-addon-ligatures": "^0.7.0",
    "xterm-addon-search": "^0.13.0",
    "xterm-addon-unicode11": "^0.6.0",
    "xterm-addon-web-links": "^0.9.0",
    "xterm-addon-webgl": "^0.16.0"
  },
  "dependencies": {
    "@achrinza/node-ipc": "^10.1.9",
    "@vue/reactivity": "^3.3.6",
    "chokidar": "^3.5.3",
    "colord": "^2.9.3",
    "file-icon": "^4.0.0",
    "fuzzaldrin-plus": "^0.6.0",
    "lodash": "^4.17.21",
    "node-pty": "^1.1.0-beta4",
    "properties": "^1.2.1",
    "semver": "^7.5.4",
    "shell-history": "^1.1.0",
    "shell-quote": "^1.8.1",
    "sudo-prompt": "^9.2.1",
    "vue": "^3.3.6",
    "yaml": "^2.3.3"
  },
  "workspaces": [
    "addons/addon-manager",
    "addons/cleaner",
    "addons/cli",
    "addons/clippy",
    "addons/editor",
    "addons/git",
    "addons/iterm2",
    "addons/l10n-ext",
    "addons/launcher",
    "addons/power-mode",
    "addons/preference",
    "addons/proxy",
    "addons/settings",
    "addons/sync",
    "addons/theme",
    "addons/updater"
  ],
  "commas:i18n": {
    "zh-CN": {
      "description": "一款可定制终端和命令执行工具"
    }
  }
}
