    "@types/lodash": "^4.14.194",
    "@types/node": "^18.16.6",
    "@types/node-ipc": "^9.2.0",
    "@types/prismjs": "^1.26.0",
    "@types/shell-quote": "^1.7.1",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue-macros/reactivity-transform": "^0.3.6",
    "canvas-confetti": "^1.6.0",
    "clippyjs": "^0.0.3",
    "devicon": "^2.15.1",
    "dotenv-cli": "^7.2.1",
    "electron": "^24.2.0",
    "electron-packager": "^17.1.1",
    "esbuild": "^0.17.18",
