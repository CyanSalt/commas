    "keyboardevent-from-electron-accelerator": "^2.0.0",
    "lucide-static": "^0.379.0",
    "png2icons": "^2.0.1",
    "release-it": "^17.3.0",
    "sass": "^1.77.2",
    "simple-icons-font": "^11.12.0",
    "stylelint": "^16.5.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vue-virtual-scroller": "^2.0.0-beta.8"
