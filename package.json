    "addons/addon-manager",
    "addons/cleaner",
    "addons/cli",
    "addons/clippy",
    "addons/editor",
    "addons/git",
    "addons/iterm2",
    "addons/l10n-ext",
    "addons/launcher",
    "addons/power-mode",
    "addons/preference",
    "addons/proxy",
    "addons/settings",
    "addons/sync",
    "addons/theme",
    "addons/updater"
